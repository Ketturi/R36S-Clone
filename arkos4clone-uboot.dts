		button-down {
			linux,code = <0x72>;
			label = "Volume Down";
			press-threshold-microvolt = <0x48058>;
		};

		button-up {
			linux,code = <0x73>;
			label = "Volume Up";
			press-threshold-microvolt = <0x3a98>;
		};
	};

